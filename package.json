  "scripts": {
    "buld:all": "nx run-many --target=build --all",
    "start:backend": "nx run backend:serve",
    "start:frontend": "nx run frontend:dev",
    "start:site": "nx run site:dev",
    "start:all": "npm-run-all --parallel start:backend start:frontend start:site",
    "build:frontend": "cross-env NODE_ENV=production nx run frontend:build",
    "build:site": "cross-env NODE_ENV=production nx run site:build",
    "build:backend": "cross-env NODE_ENV=production nx run backend:build",
    "clean": "rm -rf node_modules && rm -rf dist",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init ",
    "seed": "npx ts-node prisma/seed.ts",
    "rebuild:libs": "rm -rf dist/libs && nx run shared-models:build && nx run ui:build"
  },
  "private": true,
